{
  "title": "Switch window (disabled)",
  "rules": [
    {
      "description": "Ctrl + [] to Shift + cmd + []",
      "manipulators": [
        {
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": [
                "command",
                "shift"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": [
                "command",
                "shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}